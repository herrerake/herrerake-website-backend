{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "name": "Blog",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "richtext",
      "required": true
    },
    "body": {
      "type": "richtext",
      "required": true
    },
    "series": {
      "type": "string"
    },
    "featureImage": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "users_permissions_role": {
      "plugin": "users-permissions",
      "model": "role"
    },
    "author": {
      "via": "blogs",
      "plugin": "users-permissions",
      "model": "user"
    }
  }
}
